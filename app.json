{
  "name": "Heroku CLI Button",
  "description": "A simple app that just installs the Heroku CLI",
  "repository": "https://github.com/chap/heroku-cli-button",
  "logo": "https://node-js-sample.herokuapp.com/node.png",
  "keywords": ["heroku", "cli"],
    "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-cli"
    },
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "CLI_SETUP": {
      "required": false,
      "value": "heroku plugins:install heroku-kafka"
    }
  }
}
